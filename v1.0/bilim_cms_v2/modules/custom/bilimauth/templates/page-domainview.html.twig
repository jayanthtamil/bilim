                    class="card-ctx-btn" 
                    onclick="onCtxBtnClick(event)" 
		    data-crs="{{ crs.id }}" data-crs-name="{{ crs.title }}" data-crs-dup= "{{ crs.duplicateincompleteValues[0]['value'] }}" data-type="{{ crs.type }}" data-pid="{{ content.parent_id }}" data-display="{{ crs.display }}" data-style-id="{{ crs.style_id }}" data-framework="{{ crs.framework }}"
