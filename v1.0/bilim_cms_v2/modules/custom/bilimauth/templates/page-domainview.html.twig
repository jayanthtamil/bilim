	  </div>
	</div>


      <div class="title-container">
        <div class="title-content">
          <div class="title"><span>{{content.domaininfo.page_title}}</span></div>
          <div class="title-btns-container">
            <button class="add-folder-btn" 
            	data-id="{{content.domaininfo.page_id}}" data-type="{{content.domaininfo.page_type}}" 
            	onclick="onAddFolderClick(event)">
	            	<b>ADD</b> 
	            	<span>FOLDER</span>
            	</button>
            <button
              class="add-course-btn"
              data-id="{{content.domaininfo.page_id}}" data-type="{{content.domaininfo.page_type}}" 
              onclick="onNewCourseBtnClick(event)"
            >
            	<b>ADD</b> 
	            <span>COURSE</span>
            </button>
            <button 
            	id="click-import"
            	class="import-course-btn">
            	<b>IMPORT</b> 
	            <span>COURSE</span>
            </button>
          </div>
        </div>
      </div>
      <div class="toolbar-container">
        <div class="view-btn-group">
          <button
            name="tile"
            class="tiles-view-btn active"
            onclick="viewGroupBtnClick(event)"
          ></button>
          <button
            sname="list"
            class="list-view-btn"
            onclick="viewGroupBtnClick(event)"
          ></button>
        </div>
        <div class="sort-list-container">
          <span>Sort by:</span>
          <select class="sort-select">
            <option value="">-</option>
            <option selected="selected">Recent</option>
            <option>Alphabhetical</option>
          </select>
        </div>
      </div>
      <div class="course-lists-container custom-scrollbar">
        <div id="courseViewContainer" class="tile-view">
          <div class="list-view-header">
            <span class="list-title-header"></span>
            <span class="list-created-header">Cr√©ation</span>
            <span class="list-modified-header">Modification</span>
            <span class="list-lang-header"></span>
            <span class="list-fav-header"></span>
            <span class="list-ctx-header"></span>
          </div>
          
          {% for crs in content.courses %}
            {% if crs.type in ['domain_content_root','content_folder'] %} 
              <!-- category-card -->
              {% if crs.type == 'content_folder' %}
                  {% set cf_val = crs.id %}
                  {% set cf_root = content.domain[0].id %}
              {% else %}
                  {% set cf_root = crs.id %}
                  {% set cf_val = 0 %}
              {% endif %}
              <div class="card category-card" onclick="subfolder_view({{content.ids.domain}},{{cf_root}},{{cf_val}})" data-title="{{crs.title}}" data-changed="{{crs.changed}}">
                <div class="card-column-box"> 
                  <div class="card-title">
                    <span>{{ crs.title }}</span>
                  </div>
                  <div class="card-content">
                    <span></span>
                  </div>
                </div>
                <div class="card-created">
                  <span class="title">Created</span>
                  <span class="date">{{ crs.date }}</span>
                  <span class="time">{{ crs.time }}</span>
                </div>
                <div class="card-modified">
                  <span class="title">Last modification</span>
                  <span class="date">{{ crs.changed|date('d-m-Y') }}</span>
                  <span class="time">{{ crs.changed|date('H:i') }}</span>
                </div>
                <div class="card-lang-box">
                  <div class="card-lang-btn"></div>
                </div>
		<div id="card-fav-ctx-box">
                <div class="card-fav-box">
                  <button class="card-fav-btn"></button>
                </div>
               {# <div class="card-btn-box">&nbsp;</div>#}
               <div class="card-btn-box">
                  <button
                    class="card-ctx-btn"
                    onclick="onFolderCtxBtnClick(event)"
		    data-crs="{{ crs.id }}" data-crs-name="{{ crs.title }}" data-type="{{ crs.type }}"
                  ></button>
                </div>
		        </div>
              </div>
              <!-- end category-card -->
            {% else %}
            {% set duplicateincompleteValues = crs.duplicateincompleteValues[0]['value'] %}
              <!-- course card -->
              <div class="card course-card" data-crs="{{ crs.id }}"  data-title="{{crs.title}}" data-changed="{{crs.changed}}">
              {% if duplicateincompleteValues == '1' %}
  <div class="card-column-box" id= 'crs-update-link' data-crs-dup= "{{ crs.duplicateincompleteValues[0]['value'] }}"  >
                  <div class="card-title">
                    <span class="course-title">{{content.domaininfo.domain}}</span>
                  </div>
                  <div class="card-content">
                    <span class="desc">{{ crs.title }} </span>
                  </div>
                </div>
                {% else %}
                <div class="card-column-box" id= 'crs-update-link' data-crs-dup= "{{ crs.duplicateincompleteValues[0]['value'] }}"  onclick="onClickToEditor(event)">
                  <div class="card-title">
                    <span class="course-title">{{content.domaininfo.domain}}</span>
                  </div>
                  <div class="card-content">
                    <span class="desc">{{ crs.title }} </span>
                  </div>
                </div>
                {% endif %} 
                <div class="card-created">
                  <span class="title">Created</span>
                  <span class="date">{{ crs.date }}</span>
                  <span class="time">{{ crs.time }}</span>
                </div>
                <div class="card-modified">
                  <span class="title">Last modification</span>
                  <span class="date">{{ crs.changed|date('d-m-Y') }}</span>
                  <span class="time">{{ crs.changed|date('H:i') }}</span>
                </div>
                <div class="card-lang-box">
                	{% if crs.flag_url %}
                		<button class="card-icon flag-img-wrapper"><img class="flag-img" src={{ crs.flag_url }}></button>
                	{% else %}
                  	<button class="card-icon card-lang-icon"></button>
                  {% endif %}
                </div>
          
                <div id="card-fav-ctx-box">
                {% if duplicateincompleteValues == '1' %}
                 <div class="card-fav-box">
                </div>
                  {% else %}
                  <div class="card-fav-box">
                  {{ crs.flag_link }}
                  </div>
                {% endif %} 
               
                <div class="card-btn-box">
                  <button
                    class="card-info-btn"
                    onclick="onInfoBtnClick(event)" data-crs-id="{{crs.id}}" data-crs-dup= "{{ crs.duplicateincompleteValues[0]['value'] }}" 
                  ></button>
