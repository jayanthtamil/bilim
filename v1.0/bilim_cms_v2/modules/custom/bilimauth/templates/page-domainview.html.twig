	  <div class="show-container">
	    <div class="like-img"></div>
            <h2>COPIED WITH SUCCESS</h2>
	    <div class="success-btn-wrapper">
	      <span onclick="onClickGoTo(event)">GO TO THE FOLDER</span>
	      <span onclick="onMoveCrsPopupClose(event,'success')">STAY HERE</span>
	    </div>
	  </div>
       </div>

       <div id="dup-move-crs-container" class="move-crs-container progress-container hide">
	  <div class="backdrop"></div>
	  <div id="dup-move-crs-inner-container" class="show-container">
	  <button class="popup-close-btn" onclick="onDupMoveCrsPopupClose(event)"></button>
