          <span>MODELES</span>
        </div>
        <div class="btn-separator"></div>
        <div class="icon-button">
          <span>TESTS</span>
        </div>
      </div>
    </section>

    <section class="right-section">
    	<div class="loading-gif"></div>

	<div id="progress-container" class="progress-container hide">
	  <div class="backdrop"></div>
